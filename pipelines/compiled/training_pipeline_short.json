                  "args": [
                    "--project-id",
                    "{{$.inputs.parameters['project']}}",
                    "--source-table",
                    "{{$.inputs.parameters['source_table']}}",
                    "--direction",
                    "{{$.inputs.parameters['direction']}}",
                    "--xgb-max-depth",
                    "{{$.inputs.parameters['xgb_max_depth']}}",
                    "--learning-rate",
                    "{{$.inputs.parameters['learning_rate']}}",
